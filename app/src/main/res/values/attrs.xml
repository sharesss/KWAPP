    <declare-styleable name="CardContainer">
        <attr name="android:gravity"/>
        <attr name="orientation" format="enum">
            <enum name="ordered" value="0"/>
            <enum name="disordered" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="innerrect">
        <attr name="inner_width" format="dimension"/>
        <attr name="inner_height" format="dimension"/>
        <attr name="inner_margintop" format="dimension" />
        <attr name="inner_corner_color" format="color" />
        <attr name="inner_corner_length" format="dimension" />
        <attr name="inner_corner_width" format="dimension" />
        <attr name="inner_scan_bitmap" format="reference" />
        <attr name="inner_scan_speed" format="integer" />
        <attr name="inner_scan_iscircle" format="boolean" />
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <attr name="width_space" format="dimension"/>
        <attr name="height_space" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="Slidr">
        <attr name="slidr_barHeight" format="dimension" />
        <attr name="slidr_textTop_visible" format="boolean" />
        <attr name="slidr_textTop_size" format="dimension" />
        <attr name="slidr_textBottom_visible" format="boolean" />
        <attr name="slidr_textBottom_size" format="dimension" />
        <attr name="slidr_backgroundColor" format="color" />

